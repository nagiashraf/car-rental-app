      <trans-unit id="2802452384367461200" datatype="html">
        <source>How old do I need to be to rent a car?</source>
        <context-group purpose="location">
          <context context-type="sourcefile">src/app/features/home/faq-accordion/faq-accordion.component.ts</context>
          <context context-type="linenumber">16</context>
